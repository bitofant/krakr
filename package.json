  "scripts": {
    "test": "node test/_test.js",
    "build": "npm run webpack && echo (build 50% done) && npm run tsc && echo (build 100% done)",
    "build:prod": "NODE_ENV=production npm run build",
    "start": "NODE_ENV=development HTTP_PORT=8080 npm-run-all --parallel watch:frontend watch:backend nodemon:server",
    "watch:frontend": "webpack --watch",
    "watch:backend": "tsc --watch -p tsconfig.json",
    "nodemon:server": "nodemon dist/app.js",
    "assets": "node update-assets.js",
    "start-prod": "NODE_ENV=production HTTP_PORT=8071 node app.js",
    "forever:stop": "forever stop dist/app.js",
    "forever:start": "NODE_ENV=production HTTP_PORT=8071 forever start dist/app.js",
    "forever": "NODE_ENV=production HTTP_PORT=8071 forever restart dist/app.js",
    "webpack": "webpack",
    "tsc": "tsc",
    "typings": "typings"
  },
